{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": [
      "styled-components/native"
    ],
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}